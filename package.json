{
  "name": "igata",
  "version": "0.1.0",
  "private": false,
  "description": "igata is my boilerplate for Web Frontend",
  "repository": "git@github.com:diescake/igata.git",
  "author": "diescake",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --write './**/*.{md,js,jsx,ts,tsx,json,yml,yaml,css,sass,scss,html}'",
    "lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "style": "stylelint --fix './src/**/*.{scss}'",
    "test": "jest",
    "deploy": "echo 'Not necessary for Netlify'",
    "license": "license-checker --production --summary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add"
    ],
    "*.{scss}": [
      "yarn style",
      "git add"
    ],
    "*.{md,json,yml,yaml,css,sass,scss,html}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "7.1.0",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "@types/webpack": "^4.4.32",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prefer-arrow": "^1.1.5",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "license-checker": "^25.0.1",
    "lint-staged": "^8.2.0",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "redux-devtools": "^3.5.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.7.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "connected-react-router": "^6.4.0",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.3",
    "weak-key": "^1.0.1"
  }
}
