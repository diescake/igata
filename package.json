{
  "name": "igata",
  "version": "0.1.0",
  "private": false,
  "description": "igata is my boilerplate for Web Frontend",
  "repository": "git@github.com:diescake/igata.git",
  "author": "diescake",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --write './**/*.{md,js,jsx,ts,tsx,json,yml,yaml,css,sass,scss,html}'",
    "lint": "tslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "test": "echo 'not implemented'",
    "deploy": "echo 'not implemented'",
    "license": "license-checker --production --summary",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn test",
      "git add"
    ],
    "*.{md,json,yml,yaml,css,sass,scss,html}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "license-checker": "^25.0.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
