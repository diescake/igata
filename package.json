{
  "name": "igata",
  "version": "0.1.0",
  "private": false,
  "description": "igata is my boilerplate for Web Frontend",
  "repository": "git@github.com:diescake/igata.git",
  "author": "diescake",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --write './**/*.{md,js,jsx,ts,tsx,json,yml,yaml,css,sass,scss,html}'",
    "lint": "tslint --format verbose --fix --project tsconfig.json './src/**/*.{js,jsx,ts,tsx}'",
    "test": "echo 'not implemented'",
    "deploy": "echo 'not implemented'",
    "license": "license-checker --production --summary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn test",
      "git add"
    ],
    "*.{md,json,yml,yaml,css,sass,scss,html}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "license-checker": "^25.0.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "1.16.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
